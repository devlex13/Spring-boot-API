CREATE TABLE cursos(
    id int NOT NULL auto_increment,
    activo bool,
    profesor int(2),
    titulo varchar(50),
    nivel varchar(50),
    horas int(3),
    primary key (id)
);

CREATE TABLE profesor(
    id int NOT NULL auto_increment,
    nombre varchar(20),
    apellido1 varchar(30),
    apellido2 varchar(30),
    primary key (id)
);

